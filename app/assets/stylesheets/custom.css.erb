.video-thumb{
	position: relative;
}

.video-thumb a .glyphicon{
	color: white;
	font-size: 48px;
	text-align: center;
	position: absolute;
	top: 30%;
	left: 40%;
	opacity: 0.8;
	
	-webkit-transition: 0.5s ease;
  -moz-transition: 0.5s ease;
  -o-transition: 0.5s ease;
  transition: 0.5s ease;
}