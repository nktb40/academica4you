.kaplan_logo{
	background: url("<%= asset_path 'kaplan_logo.png' %>") no-repeat;	
	height: 100px;
}
.kaplan_back_logo{
	background: -moz-linear-gradient(top,#4547a1 0,#0f144b 100%);
	background: -webkit-gradient(linear,left top,left bottom,color-stop(0%,#4547a1),color-stop(100%,#0f144b));
	background: -webkit-linear-gradient(top,#4547a1 0,#0f144b 100%);
	background: -o-linear-gradient(top,#4547a1 0,#0f144b 100%);
	background: -ms-linear-gradient(top,#4547a1 0,#0f144b 100%);
	background: linear-gradient(to bottom,#4547a1 0,#0f144b 100%);
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#4547a1',endColorstr='#0f144b',GradientType=0);		
}

